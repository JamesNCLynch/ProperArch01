@model IEnumerable<ProperArch01.Contracts.Dto.ScheduledClassDto>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ClassTypeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClassStartTime)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ClassTypeName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ClassStartTime)
            </td>
            <td>
                @Html.ActionLink("Create", "Create", new { className = item.ClassTypeName, startTime = item.ClassStartTime})
            </td>
        </tr>
    }
</table>