@model IEnumerable<ProperArch01.Contracts.Dto.ScheduledClassDto>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ClassTypeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClassStartTime)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.ClassTypeName, "Details", "ScheduledClass", new { id = item.Id}, null)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ClassStartTime)
            </td>
            <td>
                @Html.ActionLink("Sign up", "Create", new { scid = item.Id })
            </td>
        </tr>
    }
</table>